{
  "manifest_version": 3,
  "name": "四川智慧教育平台教师专题培训自动学习插件",
  "version": "1.0",
  "description": "四川智慧教育平台教师专题培训自动学习插件可自动学习，无需手动操作。",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "inject.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://basic.sc.smartedu.cn/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}